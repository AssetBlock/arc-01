{
  "$id": "https://www.github.com/AssetBlock/arc-01/src/schemas/basic/createToken.json",
  "type": "object",
  "additionalProperties": false,
  "required": ["tknSymbol", "qty", "decPlaces", "managers"],
  "properties": {
    "txType": { "type": "string", "maximum": 5 },
    "opType": { "type": "string", "maximum": 5 },
    "tknName": { "type": "string", "maximum": 16 },
    "tknSymbol": { "type": "string", "minimum": 3, "maximum": 5 },
    "qty": { "type": "number", "minimum": 0 },
    "decPlaces": { "type": "number", "maximum": 18 },
    "managers": {
      "type": "array",
      "minItems": 1,
      "uniqueItems": true,
      "items": { "type": "string", "minimum": 58, "maximum": 58 }
    },
    "meta": { "type": "object" }
  }
}
